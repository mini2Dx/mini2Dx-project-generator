package %PACKAGE%.desktop;

import com.badlogic.gdx.backends.lwjgl.DesktopMini2DxGame;

import org.mini2Dx.libgdx.desktop.DesktopMini2DxConfig;

import %PACKAGE%.%MAIN_CLASS%;

public class DesktopLauncher {
	public static void main (String[] arg) {
		DesktopMini2DxConfig config = new DesktopMini2DxConfig(%MAIN_CLASS%.GAME_IDENTIFIER);
		config.vSyncEnabled = true;
		new DesktopMini2DxGame(new %MAIN_CLASS%(), config);
	}
}
